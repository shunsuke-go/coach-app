<%= provide(:title,"ユーザー 一覧") %>
<div class="row">
  <div class="col l8 push-l2">
    <ul class="tabs">
      <li class="tab col l6"><a href="#test1"><h5 class="message-tabs">ユーザーを探す</h5></a></li>
      <li class="tab col l6"><a href="#test2"><h5 class="message-tabs">コーチを探す</h5></a></li>
    </ul>
  </div>
  <div id="test1" class="col s12">
    <div class="row">
      <div class="col s12 m12 l8 push-l2">
        <div class="card">
          <ul class="users">
            <%= render @users %>
          </ul>
        </div>
      </div>
    </div>  
    <div class="center"><%= will_paginate @users%></div>
  </div>
  <div id="test2" class="col s12">
    <div class="row">
      <div class="col s12 m12 l8 push-l2">       
        <ul class="users">
          <%= render @coaches %>
        </ul>        
      </div>
    </div>  
  </div>
  <div class="center"><%= will_paginate @coaches%></div>
</div>



<script>
  document.addEventListener('DOMContentLoaded', function() {
  var tabs = document.querySelectorAll('.tabs');
  var instance = M.Tabs.init(tabs,[]);
  });
</script>
